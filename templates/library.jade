script#library-template(type="text/template")
  table
    tr
      td(colspan=14) <%= name %>
      |    <% i = 0; _.each(actions, function(action, name) { %>
      |      <td style="padding-left: 5px; padding-top: 5px">
      |        <%= Graphic.fromAction(action, 32, 32).svg() %>
      |      </td>
      |      <td style="padding-right: 5px; padding-top: 10px">
      |        <%= name %>
      |      </td>
      |      <% if ((++i + 7) % 10 == 0) { %>
      |        </tr><tr>
      |      <% } %>
      |    <% }); %>
